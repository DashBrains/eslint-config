{
  "name": "@dashbrains/eslint-config",
  "version": "1.0.5",
  "main": "index.js",
  "license": "MIT",
  "description": "Eslint configuration used by DashBrains",
  "keywords": [
    "eslint",
    "config",
    "dashbrains"
  ],
  "author": "DashBrains Engineering <contact@dashbrains.dev>",
  "homepage": "https://github.com/DashBrains/eslint-config#readme",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/DashBrains/eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bugs": {
    "url": "https://github.com/DashBrains/eslint-config/issues"
  },
  "scripts": {
    "lint:fix": "eslint '**/*.{js,jsx,ts,tsx}' --fix --no-error-on-unmatched-pattern",
    "format:fix": "prettier '**/*.{js,jsx,ts,tsx}' --write --no-error-on-unmatched-pattern",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}' --no-error-on-unmatched-pattern",
    "format": "prettier '**/*.{js,jsx,ts,tsx}' --check --no-error-on-unmatched-pattern",
    "prepare": "husky install"
  },
  "dependencies": {
    "@eslint/compat": "^1.1.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.1.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^4.1.3",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "husky": "^9.1.5"
  }
}
